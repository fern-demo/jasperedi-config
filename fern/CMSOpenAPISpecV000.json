    "components": {
        "schemas": {
            "EligibilityInquiryRequest": {
                "required": [
                    "Detail",
                    "Header"
                ],
                "type": "object",
                "properties": {
                    "Detail": {
                        "required": [
                            "ResponsibleReportingEntity",
                            "Subscriber"
                        ],
                        "type": "object",
                        "properties": {
                            "Reference": {
                                "oneOf": [
                                    {
                                        "type": "string"
                                    },
                                    {
                                        "maxLength": 2,
                                        "minLength": 2,
                                        "type": "string",
                                        "enum": [
                                            "IG",
                                            "NQ",
                                            "EA"
                                        ]
                                    }
                                ]
                            },
                            "ResponsibleReportingEntity": {
                                "maxLength": 9,
                                "minLength": 9,
                                "type": "string",
                                "description": "9-digit Responsible\nReporting Entity (RRE)\nnumber assigned by\nCOB.\nThe RRE number must\ncontain 9 digits.\nPopulate leading\npositions with zeros."
                            },
                            "Subscriber": {
                                "required": [
                                    "Birthdate",
                                    "FirstName",
                                    "Gender",
                                    "LastName",
                                    "MedicareIDMember’s Medicare ID (HICN or MBI)"
                                ],
                                "type": "object",
                                "properties": {
                                    "Birthdate": {
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "FirstName": {
                                        "type": "string"
                                    },
                                    "Gender": {
                                        "maxLength": 1,
                                        "minLength": 1,
                                        "type": "string",
                                        "enum": [
                                            "M",
                                            "F"
                                        ]
                                    },
                                    "LastName": {
                                        "type": "string"
                                    },
                                    "MedicareIDMember’s Medicare ID (HICN or MBI)": {
                                        "type": "string",
                                        "description": "Member’s Medicare ID\n(HICN or MBI)"
                                    }
                                }
                            }
                        }
                    },
                    "Header": {
                        "type": "object",
                        "properties": {
                            "CreatedDateTime": {
                                "type": "string",
                                "format": "date-time"
                            }
                        }
                    },
                    "Meta": {
                        "type": "object",
                        "properties": {
                            "ReceivedDateTime": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "Settings": {
                                "type": "object",
                                "properties": {
                                    "favorite": {
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "imported": {
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "readunread": {
                                        "type": "string",
                                        "format": "date-time"
                                    }
                                }
                            },
                            "Tags": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "id": {
                                "type": "string"
                            },
                            "source": {
                                "type": "string"
                            }
                        }
                    }
                }
            },
            "EligibilityResponseResponse": {
                "required": [
                    "Detail",
                    "Header"
                ],
                "type": "object",
                "properties": {
                    "Detail": {
                        "required": [
                            "ResponsibleReportingEntity",
                            "Subscriber"
                        ],
                        "type": "object",
                        "properties": {
                            "OriginatingCompanyID": {
                                "type": "number",
                                "description": "The number ‘9’ plus the\nRRE number. The RRE\nnumber must contain 9\ndigits. Populate leading\npositions with zeros.",
                                "format": "double"
                            },
                            "PlanEligibilitys": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "EndDate": {
                                            "type": "string",
                                            "format": "date"
                                        },
                                        "MedicarePart": {
                                            "type": "string",
                                            "enum": [
                                                "A",
                                                "B",
                                                "C"
                                            ]
                                        },
                                        "PartC": {
                                            "type": "object",
                                            "properties": {
                                                "ContractNumber": {
                                                    "type": "string"
                                                },
                                                "PBPNumber": {
                                                    "type": "string"
                                                }
                                            }
                                        },
                                        "StartDate": {
                                            "type": "string",
                                            "format": "date"
                                        }
                                    }
                                }
                            },
                            "References": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "RefQual": {
                                            "maxLength": 2,
                                            "minLength": 2,
                                            "type": "string",
                                            "enum": [
                                                "IG",
                                                "NQ",
                                                "EA"
                                            ]
                                        },
                                        "Reference": {
                                            "type": "string"
                                        }
                                    }
                                }
                            },
                            "RejectionReason": {
                                "type": "string",
                                "enum": [
                                    "Subscriber or Insured not found",
                                    "Duplicate Subscriber or Insured ID"
                                ]
                            },
                            "ResponsibleReportingEntity": {
                                "maxLength": 9,
                                "minLength": 9,
                                "type": "string",
                                "description": "9-digit Responsible\nReporting Entity (RRE)\nnumber assigned by\nCOB.\nThe RRE number must\ncontain 9 digits.\nPopulate leading\npositions with zeros."
                            },
                            "Subscriber": {
                                "required": [
                                    "Birthdate",
                                    "FirstName",
                                    "Gender",
                                    "LastName",
                                    "MedicareID"
                                ],
                                "type": "object",
                                "properties": {
                                    "Birthdate": {
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "FirstName": {
                                        "type": "string"
                                    },
                                    "Gender": {
                                        "maxLength": 1,
                                        "minLength": 1,
                                        "type": "string",
                                        "enum": [
                                            "M",
                                            "F"
                                        ]
                                    },
                                    "LastName": {
                                        "type": "string"
                                    },
                                    "MedicareID": {
                                        "type": "string",
                                        "description": "Member’s Medicare ID\n(HICN or MBI)"
                                    }
                                }
                            }
                        }
                    },
                    "Header": {
                        "type": "object",
                        "properties": {
                            "CreatedDateTime": {
                                "type": "string",
                                "format": "date-time"
                            }
                        }
                    },
                    "Meta": {
                        "type": "object",
                        "properties": {
                            "ReceivedDateTime": {
                                "type": "string",
                                "format": "date-time"
                            },
                            "Settings": {
                                "type": "object",
                                "properties": {
                                    "favorite": {
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "imported": {
                                        "type": "string",
                                        "format": "date-time"
                                    },
                                    "readunread": {
                                        "type": "string",
                                        "format": "date-time"
                                    }
                                }
                            },
                            "Tags": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "id": {
                                "type": "string"
                            },
                            "source": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
